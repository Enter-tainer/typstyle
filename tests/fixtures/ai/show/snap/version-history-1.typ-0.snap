// Timeline
#for (
  i,
  (
    version,
    date,
    commit,
  ),
) in version-data.enumerate() {
  let version-type = get-version-type(
    version,
  )
  let color = version-colors.at(
    version-type,
  )
  let icon = version-icons.at(
    version-type,
  )

  // Timeline entry
  grid(
    columns: (
      auto,
      1fr,
      auto,
    ),
    column-gutter: 1cm,
    align: (
      left,
      left,
      right,
    ),

    // Version badge
    [
      #box(
        fill: color,
        radius: 4pt,
        inset: (
          x: 8pt,
          y: 4pt,
        ),
      )[
        #text(
          fill: white,
          weight: "bold",
          size: 10pt,
        )[#version]
      ]
    ],

    // Version info
    [
      #text(
        size: 14pt,
        weight: "bold",
        fill: rgb(
          "#1f2937",
        ),
      )[
        #icon #version
      ]
      #linebreak()
      #text(
        size: 10pt,
        fill: rgb(
          "#6b7280",
        ),
      )[
        Released on #date
      ]
      #if (
        version-type
          == "minor"
      ) [
        #linebreak()
        #text(
          size: 9pt,
          fill: color,
          style: "italic",
        )[
          Major feature release
        ]
      ] else if (
        version-type
          == "rc"
      ) [
        #linebreak()
        #text(
          size: 9pt,
          fill: color,
          style: "italic",
        )[
          Release candidate
        ]
      ]
    ],

    // Commit hash
    [
      #box(
        fill: rgb(
          "#f3f4f6",
        ),
        radius: 3pt,
        inset: (
          x: 6pt,
          y: 3pt,
        ),
      )[
        #text(
          size: 9pt,
          font: "JetBrains Mono",
          fill: rgb(
            "#374151",
          ),
        )[
          #commit
        ]
      ]
    ],
  )

  // Connecting line (except for last item)
  if (
    i
      < version-data.len()
        - 1
  ) [
    #v(
      0.3cm,
    )
    #pad(
      left: 2.5cm,
