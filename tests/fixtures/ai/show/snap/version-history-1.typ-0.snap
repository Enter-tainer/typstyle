#let get-version-type(
  version,
) = {
  if version.contains(
    "rc",
  ) {
    "rc"
  } else if (
    version.match(
      regex(
        "v\d+\.\d+\.0$",
      ),
    )
      != none
  ) {
    "minor"
  } else {
    "patch"
  }
}

